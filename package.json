{
  "name": "tab-web-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start -p $PORT",
    "test": "jest --testMatch \"**/*.test.(j|t)s?(x)\"",
    "test:watch": "yarn test --watch",
    "integration": "jest --testMatch \"**/*.spec.(j|t)s?(x)\" --setupFilesAfterEnv ./jest-setup-integration-tests.ts",
    "integration:watch": "yarn integration --watch",
    "ts:watch": "tsc --watch -p ./tsconfig.json",
    "ts:build": "tsc -p ./tsconfig.json",
    "lint": "next lint --no-cache",
    "prettier": "prettier --write .",
    "prepare": "husky install",
    "storybook": "start-storybook -s ./public -p 9009",
    "build-storybook": "build-storybook",
    "ssl-proxy": "npx local-ssl-proxy --key ./local.dev.app.tap3d.com-key.pem --cert ./local.dev.app.tap3d.com.pem --source 3001 --target 3000",
    "db:start": "docker compose --env-file .env up -d",
    "db:stop": "docker stop tap-db",
    "db:destroy": "docker compose --env-file .env down",
    "db:migrate": "yarn knex migrate:latest",
    "db:migrate:refresh": "yarn knex migrate:rollback --all && yarn knex migrate:latest",
    "db:seed": "yarn knex seed:run",
    "knex": "npx ts-node -T -r dotenv/config ./node_modules/knex/bin/cli.js --knexfile backend/services/db/knexfile.ts --esm"
  },
  "dependencies": {
    "@aws-sdk/client-lambda": "^3.346.0",
    "@aws-sdk/client-sqs": "^3.267.0",
    "@headlessui/react": "^1.7.4",
    "@heroicons/react": "^2.0.13",
    "@next/font": "^13.0.2",
    "@propelauth/express": "^2.0.0",
    "@propelauth/react": "^2.0.9",
    "@segment/analytics-next": "^1.53.0",
    "@sentry/nextjs": "^7.57.0",
    "@storybook/addon-storyshots": "^6.5.13",
    "@types/knex": "^0.16.1",
    "@xapi/xapi": "^2.1.2",
    "axios": "1.1.3",
    "classnames": "^2.3.2",
    "cookie": "^0.5.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "d3": "^7.6.1",
    "dotenv": "^16.0.3",
    "knex": "^2.4.2",
    "lodash": "^4.17.21",
    "lodash.groupby": "^4.6.0",
    "luxon": "^3.1.0",
    "nanoid": "^4.0.0",
    "next": "13.3.3",
    "next-api-decorators": "2.0.1-beta.2",
    "path-to-regexp": "^6.2.1",
    "pg": "^8.8.0",
    "query-string": "^7.1.1",
    "raw-body": "^2.5.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "resize-observer-polyfill": "^1.5.1",
    "sharp": "^0.32.3",
    "snake-camel": "^1.0.8",
    "sqlite3": "^5.1.4",
    "storybook-addon-next-router": "^4.0.1",
    "swr": "^2.1.3",
    "tiny-invariant": "^1.3.1",
    "uuid": "^9.0.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@sentry/cli": "^2.19.4",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.5.13",
    "@storybook/manager-webpack5": "^6.5.13",
    "@storybook/react": "^6.5.13",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/crypto-js": "^4.1.1",
    "@types/d3": "^7.4.0",
    "@types/lodash.groupby": "^4.6.7",
    "@types/luxon": "^3.1.0",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "@types/uuid": "^9.0.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.3.0",
    "cross-env": "^7.0.3",
    "eslint": "8.27.0",
    "eslint-config-next": "13.0.2",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "lint-staged": "^13.1.0",
    "local-ssl-proxy": "^1.3.0",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.2.3",
    "ts-node": "^10.9.1",
    "typescript": "4.8.4"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint"
    ],
    "*.tsx": [
      "prettier --write",
      "eslint"
    ],
    "*.js": [
      "prettier --write",
      "eslint"
    ],
    "*.json": [
      "prettier --write",
      "next lint --file"
    ]
  },
  "engines": {
    "node": "18.*",
    "yarn": "1.*"
  }
}
